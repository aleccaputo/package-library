// need this file so that eslint cli doesnt explode
{
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true,
    "jasmine": true
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "extends": "./index.js",
  "rules": {
    // these rules are turned off so that the eslint json is easliy copy-paste-able to a real .eslintrc file
    "quotes": "off",
    "quote-props": "off"
  }
}
