// need this file so that eslint cli doesnt explode
{
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true,
    "jasmine": true
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "extends": "./index.js",
  "rules": {
    // these rules are turned off so that the eslint JSON is valid JSON
    "@typescript-eslint/quotes": ["error", "double"],
    "@typescript-eslint/comma-dangle": ["error", "never"],
    "quote-props": "off"
  }
}
